{
  "name": "fluxo-cart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn dev:next\" \"yarn dev:json-server\"",
    "dev:next": "next dev",
    "dev:json-server": "json-server --watch server.json --port 3001",
    "build": "next build",
    "start": "concurrently \"next start\" \"yarn dev:json-server\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "axios": "^1.6.7",
    "framer-motion": "^11.0.3",
    "json-server": "^1.0.0-alpha.23",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/parse-json": "^4.0.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "typescript": "^5"
  }
}
